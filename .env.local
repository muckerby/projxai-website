# Environment variables for ProjxAI Website
# Copy this file to .env.local and fill in your actual values

# Algolia Search Configuration
# Get these values from your Algolia dashboard
NEXT_PUBLIC_ALGOLIA_APP_ID=your_algolia_app_id_here
NEXT_PUBLIC_ALGOLIA_INDEX_NAME=your_index_name_here
NEXT_PUBLIC_ALGOLIA_SEARCH_API_KEY=your_search_api_key_here

# Algolia Admin API Key (for indexing, keep this secret)
ALGOLIA_ADMIN_API_KEY=your_admin_api_key_here

# Stackbit Preview (optional)
STACKBIT_PREVIEW=false
